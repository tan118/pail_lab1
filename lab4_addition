section .data
    sum db 0          ; byte to store ASCII digit

section .text
    global _start

_start:
    mov ax, 8
    add ax, 1         ; ax = 9

    ; Convert number to ASCII by adding '0'
    add al, '0'
    mov [sum], al     ; store ASCII character in sum

    ; write syscall: sys_write(1, sum, 1)
    mov eax, 4        ; sys_write
    mov ebx, 1        ; stdout
    mov ecx, sum      ; address of sum
    mov edx, 1        ; length 1 byte
    int 0x80

    ; exit syscall: sys_exit(0)
    mov eax, 1
    mov ebx, 0
    int 0x80
